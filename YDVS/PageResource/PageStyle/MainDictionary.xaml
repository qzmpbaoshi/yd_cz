<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="ModalColor" Color="#A1E8E8EC"/>
    <SolidColorBrush x:Key="BorderColor" Color="SkyBlue"/>
    <SolidColorBrush x:Key="MainColor" Color="#0078D7"/>
    <SolidColorBrush x:Key="ContentColor" Color="Lavender"/>
    <SolidColorBrush x:Key="MouseOverColor" Color="#5CD5F6"/>
    <SolidColorBrush x:Key="UnEnabledColor" Color="#7EAAEB"/>
    <SolidColorBrush x:Key="CheckedColor" Color="#399BFF"/>
    <SolidColorBrush x:Key="TableHeadColor" Color="#0091EA"/>
    <SolidColorBrush x:Key="TableBodyColor" Color="Lavender"/>
    <SolidColorBrush x:Key="TableHeadBorderColor" Color="White"/>
    <SolidColorBrush x:Key="TableBodyBorderColor" Color="LightGray"/>
    <SolidColorBrush x:Key="TableFootColor" Color="#0091EA"/>
    <FontFamily x:Key="MainFontFamily">Microsoft YaHei</FontFamily>
    <SolidColorBrush x:Key="TextBlockFontColor" Color="#15428B"/>
    
</ResourceDictionary>